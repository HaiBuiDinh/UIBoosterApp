<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyLauncher" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >#ECF0F9</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
        <item name="android:navigationBarColor">#ECF0F9</item>
    </style>


    <style name="NavigationTheme" parent="Theme.AppCompat">
        <item name="itemIconTint">@android:color/white</item>
        <item name="itemTextColor">@android:color/white</item>
        <item name="android:background">#00ffffff</item>
    </style>

    <style name="ListDivider">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">3px</item>
        <item name="android:background">#E4E4E4</item>
    </style>
</resources>